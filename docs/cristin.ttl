@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix cr:     <http://api.cristin.no/> .

#Info about rdfs types
#https://www.w3.org/TR/rdf-schema/#ch_resource

cr:hasExactlyOne
    rdf:type    rdf:Property ;
    rdfs:label  "property with exactly one value" ;
    rdfs:range  rdfs:Resource ;
    rdfs:domain cr:Result .

cr:Result
    rdf:type         rdfs:Class ;
    cr:hasExactlyOne cr:Id ;
    cr:hasExactlyOne cr:arstall ;
    cr:hasExactlyOne cr:dato_opprettet ;
    cr:hasExactlyOne cr:dato_opprettet ;
    cr:hasExactlyOne cr:varbeidhovedkatkode ;
    cr:hasExactlyOne cr:varbeidunderkatkode .

cr:Id
    rdf:type    rdf:Property ;
    rdfs:label  "Identifier of Cristin Result entry" ;
    rdfs:range  xsd:positiveInteger ;
    rdfs:domain cr:Result .

cr:arstall
    rdf:type    rdf:Property ;
    rdfs:label  "The year a resource (result) was published" ;
    rdfs:range  xsd:positiveInteger ;
    rdfs:domain cr:Result .

cr:dato_opprettet
    rdf:type    rdf:Property ;
    rdfs:label  "The date the resource was registered in Cristin" ;
    rdfs:range  xsd:date ;
    rdfs:domain cr:Result .

cr:varbeidhovedkatkode
    rdf:type    rdf:Property ;
    rdfs:label  "The main category of the resource" ;
    rdfs:comment
                "The range of this property is found here: https://api.cristin.no/v2/results/categories?lang=nb,en" ;
    rdfs:range  xsd:string ;
    rdfs:domain cr:Result .

cr:varbeidunderkatkode
    rdf:type     rdf:Property ;
    rdfs:label   "The secondary category of the resource" ;
    rdfs:comment "No specific range available at the moment" ;
    rdfs:range   xsd:string ;
    rdfs:domain  cr:Result .


